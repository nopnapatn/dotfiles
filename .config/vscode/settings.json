{
  "workbench.startupEditor": "none",
  "workbench.editor.labelFormat": "short",
  "workbench.layoutControl.enabled": true,
  "workbench.productIconTheme": "el-vsc-v1-icons",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.preferredDarkColorTheme": "paddy-dark",
  "workbench.iconTheme": null,
  "editor.fontFamily": "MesloLGS NF",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 5,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.stickyScroll.enabled": false,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.suggest.insertMode": "replace",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit"
  },
  "explorer.compactFolders": false,
  "symbols.hidesExplorerArrows": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": false,
  "prettier.semi": false,
  "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": false,
  "prettier.trailingComma": "none",
  "vim.foldfix": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["leader", "x"],
      "commands": [":x"]
    },
    {
      "before": ["leader", "w"],
      "commands": [":w"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["s", "x"],
      "commands": [":x"]
    },
    {
      "before": ["s", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["s", "v"],
      "commands": [":vsp"]
    },
    {
      "before": ["s", "s"],
      "commands": [":sp"]
    },
    {
      "before": ["s", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["s", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["s", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["s", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["t", "n"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["t", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["leader", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["leader", "i"],
      "commands": ["turboConsoleLog.displayLogMessage"]
    },
    {
      "before": ["leader", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["leader", "e"],
      "commands": ["workbench.files.action.focusFilesExplorer"],
      "when": ["editorTextFocus"]
    }
  ],
  "console-ninja.featureSet": "Community",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.lint.unknownAtRules": "ignore",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "go.toolsManagement.autoUpdate": true,
  "[tailwindcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "terminal.integrated.env.osx": {},
  "sonarlint.rules": {
    "typescript:S6638": {
      "level": "on"
    },
    "typescript:S6851": {
      "level": "on"
    }
  },
  "makefile.configureOnOpen": true,
  "window.autoDetectColorScheme": true,
  "solidity.telemetry": true
}
